# Track font binaries with Git LFS to avoid bloating the repository history.
assets/fonts/*.otf filter=lfs diff=lfs merge=lfs -text
assets/fonts/*.ttf filter=lfs diff=lfs merge=lfs -text
assets/videos/**/*.mp4 filter=lfs diff=lfs merge=lfs -text
assets/videos/**/*.mov filter=lfs diff=lfs merge=lfs -text